{
  "name": "@db-astro-suite/starwizz",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /db-astro-suite/starwizz/ && pnpm postbuild",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postbuild": "cp dist/starwizz/browser/index.html dist/starwizz/browser/404.html"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "21.1.2",
    "@angular/compiler": "21.1.2",
    "@angular/core": "21.1.2",
    "@angular/forms": "21.1.2",
    "@angular/platform-browser": "21.1.2",
    "@angular/router": "21.1.2",
    "@tailwindcss/postcss": "4.1.18",
    "postcss": "8.5.6",
    "rxjs": "7.8.2",
    "tailwindcss": "4.1.18",
    "tslib": "2.8.1",
    "zone.js": "0.15.1",
    "@db-astro-suite/theme": "workspace:*"
  },
  "devDependencies": {
    "@angular/build": "21.1.2",
    "@angular/cli": "21.1.2",
    "@angular/compiler-cli": "21.1.2",
    "@types/jasmine": "5.1.15",
    "jasmine-core": "5.9.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.9.3"
  }
}
